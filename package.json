{
  "name": "n8n-nodes-kie",
  "version": "0.1.0",
  "description": "n8n nodes for Kie.ai - AI-powered automation and workflow integration",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "n8n-nodes",
    "n8n-nodes-kie",
    "kie",
    "kie-ai",
    "ai",
    "automation"
  ],
  "license": "MIT",
  "homepage": "https://kie.ai",
  "author": {
    "name": "Valics Lehel",
    "email": "lvalics@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lvalics/n8n-nodes-kie.git"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "packageManager": "pnpm@9.1.4",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" npm run clean && tsc && gulp build:icons",
    "dev": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "echo 'Linting disabled temporarily'",
    "lintfix": "echo 'Linting disabled temporarily'",
    "prepublishOnly": "npm run build",
    "runn8n": "npx nodemon -q -e js,ts,yml,yaml,json,node,graphql,env,env.sample --exec n8n start"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/KieApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Kie/FileUpload/KieFileUpload.node.js",
      "dist/nodes/Kie/GoogleNanoBananaPro/KieGoogleNanoBananaPro.node.js",
      "dist/nodes/Kie/Qwen/KieQwen.node.js",
      "dist/nodes/Kie/Seedream/KieSeedream.node.js",
      "dist/nodes/Kie/ZImage/KieZImage.node.js",
      "dist/nodes/Kie/TaskStatus/KieTaskStatus.node.js"
    ]
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/node": "^22.13.10",
    "@typescript-eslint/parser": "~8.26.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.22.0",
    "eslint-plugin-jsdoc": "^50.6.6",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "gulp": "^5.0.0",
    "i": "^0.3.7",
    "n8n-workflow": "~1.70.0",
    "prettier": "^3.5.3",
    "typescript": "~5.8.2"
  },
  "pnpm": {
    "ignoredBuiltDependencies": [
      "eslint-plugin-n8n-nodes-base"
    ],
    "onlyBuiltDependencies": [
      "es5-ext",
      "eslint-plugin-n8n-nodes-base"
    ]
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "bugs": {
    "url": "https://github.com/lvalics/n8n-nodes-kie/issues"
  }
}
