{
  "name": "n8n-nodes-kie",
  "version": "0.2.0",
  "description": "n8n nodes for Kie.ai - AI image generation, video creation, image upscaling with Seedream, Flux-2 Pro, Qwen, Google Nano Banana Pro, Topaz, Recraft, and Infinitalk models",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "n8n-nodes",
    "n8n-nodes-kie",
    "kie",
    "kie-ai",
    "ai",
    "automation",
    "image-generation",
    "text-to-image",
    "image-to-image",
    "image-upscaling",
    "video-generation",
    "video-upscaling",
    "text-to-video",
    "image-to-video",
    "video-to-video",
    "audio-to-video",
    "talking-head",
    "seedream",
    "flux",
    "flux-2",
    "qwen",
    "z-image",
    "google-nano-banana-pro",
    "grok",
    "grok-imagine",
    "bytedance",
    "seedance",
    "seedance-1-5-pro",
    "hailuo",
    "hailuo-02",
    "wan",
    "wan-2-6",
    "sora",
    "sora-2",
    "veo",
    "veo3",
    "veo-3",
    "google-veo",
    "character-animation",
    "topaz",
    "recraft",
    "infinitalk",
    "ai-models",
    "image-enhancement",
    "workflow",
    "stable-diffusion"
  ],
  "license": "MIT",
  "homepage": "https://kie.ai",
  "author": {
    "name": "Valics Lehel",
    "email": "lvalics@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lvalics/n8n-nodes-kie.git"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "packageManager": "pnpm@9.1.4",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" npm run clean && tsc && gulp build:icons",
    "dev": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "echo 'Linting disabled temporarily'",
    "lintfix": "echo 'Linting disabled temporarily'",
    "prepublishOnly": "npm run build",
    "runn8n": "npx nodemon -q -e js,ts,yml,yaml,json,node,graphql,env,env.sample --exec n8n start"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/KieApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Kie/FileUpload/KieFileUpload.node.js",
      "dist/nodes/Kie/Flux2Pro/KieFlux2Pro.node.js",
      "dist/nodes/Kie/GoogleNanoBananaPro/KieGoogleNanoBananaPro.node.js",
      "dist/nodes/Kie/GrokImagine/KieGrokImagine.node.js",
      "dist/nodes/Kie/Hailuo/KieHailuo.node.js",
      "dist/nodes/Kie/ImageUpscaler/KieImageUpscaler.node.js",
      "dist/nodes/Kie/Infinitalk/KieInfinitalk.node.js",
      "dist/nodes/Kie/Qwen/KieQwen.node.js",
      "dist/nodes/Kie/Seedance/KieSeedance.node.js",
      "dist/nodes/Kie/Seedream/KieSeedream.node.js",
      "dist/nodes/Kie/Sora2Characters/KieSora2Characters.node.js",
      "dist/nodes/Kie/Sora2Pro/KieSora2Pro.node.js",
      "dist/nodes/Kie/Sora2ProStoryboard/KieSora2ProStoryboard.node.js",
      "dist/nodes/Kie/SoraWatermarkRemover/KieSoraWatermarkRemover.node.js",
      "dist/nodes/Kie/Veo3/KieVeo3.node.js",
      "dist/nodes/Kie/VideoUpscaler/KieVideoUpscaler.node.js",
      "dist/nodes/Kie/Wan26/KieWan26.node.js",
      "dist/nodes/Kie/ZImage/KieZImage.node.js",
      "dist/nodes/Kie/TaskStatus/KieTaskStatus.node.js"
    ]
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/node": "^22.13.10",
    "@typescript-eslint/parser": "~8.26.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.22.0",
    "eslint-plugin-jsdoc": "^50.6.6",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "gulp": "^5.0.0",
    "i": "^0.3.7",
    "n8n-workflow": "~1.70.0",
    "prettier": "^3.5.3",
    "typescript": "~5.8.2"
  },
  "pnpm": {
    "ignoredBuiltDependencies": [
      "eslint-plugin-n8n-nodes-base"
    ],
    "onlyBuiltDependencies": [
      "es5-ext",
      "eslint-plugin-n8n-nodes-base"
    ]
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "bugs": {
    "url": "https://github.com/lvalics/n8n-nodes-kie/issues"
  }
}
